{
  "name": "trpc",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/trpc/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/trpc",
        "main": "libs/trpc/src/index.ts",
        "tsConfig": "libs/trpc/tsconfig.lib.json",
        "assets": [
          "libs/trpc/*.md",
          {
            "input": "./libs/trpc/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/trpc/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/trpc",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/trpc",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/trpc/**/*.ts",
          "libs/trpc/generators.json",
          "libs/trpc/executors.json",
          "libs/trpc/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/trpc/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
